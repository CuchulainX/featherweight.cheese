TIMEFORMAT=$'  real %3lR    user %3lU    sys\t%3lS'
BEFORE=/dev/null
AFTER=/dev/null

src1() {
  pngtopnm testdata/stripes.png | pnmdepth 255
}

src2() {
  jpegtopnm testdata/code.jpg 2> /dev/null | pnmdepth 255 | pgmtoppm white
}

exercise() {
  time src1 | tee $BEFORE | eval $1 > $AFTER
  time src2 | tee $BEFORE | eval $1 > $AFTER
}


echo "No-op"
exercise cat

echo "Detect"
exercise ./detect.py

echo "Viewer"
exercise ./viewer.py

echo "Phase"
exercise ./phase.py 

echo "Split"
exercise ./split.py

echo "Wobble"
exercise ./wobble.py

echo "ALL"
exercise "./wobble.py | ./viewer.py | ./split.py"

LOC=$(cat *.py | grep -v "^#" | wc -l)
echo -e "\nBrought to you by $LOC lines of python."







